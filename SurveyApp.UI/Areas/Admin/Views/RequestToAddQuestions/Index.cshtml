@{
    Layout = "_Layout";
}
@model List<QuestionDTO>
<div class=" text-center">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Question</th>
                <th>Choices</th>
                <th>Operations</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var question in Model)
            {
                <tr>
                    <td>@question.Text</td>
                    <td>
                        <ul>


                            @foreach (var choice in question.Choices)
                            {
                                <li>@choice</li>

                            }
                        </ul>
                    </td>
                    <td>.</td>
                </tr>
            }
        </tbody>
    </table>
</div>