@model List<SurveyDTO>
<div>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" />
    <div class="display-6">My Surveys</div>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Survey Id</th>
                <th>Result</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var survey in Model)
            {
                <tr>
                    <td class="text-center">@survey.Id</td>
                    <td><a class="btn btn-outline-success" href="/Survey/SurveyResults/@survey.Id"> <i class="fa fa-list"></i> Survey Result</a>
                </td>
                    <td>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
